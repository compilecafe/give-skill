{
  "name": "sena",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "bun --filter '*' dev",
    "dev:web": "bun --filter 'web' dev",
    "build:web": "bun --filter 'web' build",
    "build:sena": "bun --filter 'sena' build",
    "build": "bun --filter '*' build",
    "lint": "bun --filter '*' lint",
    "fmt": "bun --filter '*' fmt",
    "lint:sena": "bun --filter 'sena' lint",
    "fmt:sena": "bun --filter 'sena' fmt",
    "lint:web": "bun --filter 'web' lint",
    "fmt:web": "bun --filter 'web' fmt"
  }
}